detect_model_equals = \
	lambda model, detection, *input: \
		model(*input) == detection

is_deepfake = lambda model, data: \
	lambda *args: detect_model_equals \
		(model, data, *args)


factor_model = lambda a, b: a * b


regenerative_model = lambda *data: \
	return lambda *args: \
		regenerative_model \
			(*(data + args))


lambda prompt = 'Tell me what others want to know.': \
	assertEquals(is_deepfake(factor_model,
		factor_model(prompt, 1))
			(prompt, 1), True)
