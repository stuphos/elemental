detect_model_equals = \
	lambda model, detection, *input: \
		model(*input) == detection

is_deepfake = lambda model, data: \
	lambda *args: detect_model_equals \
		(model, data, *args)


factor_model = lambda a, b: a * b


assertEquals \
	(is_deepfake(factor_model, factor_model('Tell me what others want to know.', 1))
		('Tell me what others want to know.', 1),
	 True)
